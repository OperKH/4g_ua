    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-vue": "^5.2.2"
  },
  "engines": {
    "node": ">= 12.1.0",
    "npm": ">= 6.9.0"
  }
