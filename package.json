{
  "name": "4g-ua",
  "version": "1.0.0",
  "description": "4G UA",
  "author": "Alexander Zhytnyk <oper-88@mail.ru>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "prod": "nuxt start",
    "start": "npm i && npm run crawler && npm run build && concurrently \"npm run cron\" \"npm run prod\"",
    "generate": "nuxt generate",
    "crawler": "node --experimental-modules cron/crawlerRunner.mjs",
    "cron": "node --experimental-modules cron/index.mjs",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "node-cron": "^1.2.1",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.0.0"
  }
}
